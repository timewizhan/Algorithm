import java.util.Random;



public class Fence {
	private final static int BOARD_COUNT = 5;
	private final static int NUM_INITLAL = 0;
	private final static int TEST_BED_COUNT = 1;
	private final static int BAR_HEIGHT = 10;
	
	public static void main(String[] args) {
		/* */
		generateRandom(BOARD_COUNT, NUM_INITLAL);
	}
	
	public static int generateRandom(int randomRange, int initialVal) {
		Random random = new Random();
		int randomNum = random.nextInt(randomRange) + initialVal;
		
		return randomNum;
	}
}
