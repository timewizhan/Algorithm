import java.util.Random;
import java.util.ArrayList;


public class Fence {
	private final static int BOARD_COUNT = 5;
	private final static int NUM_INITLAL = 0;
	private final static int TEST_BED_COUNT = 1;
	private final static int BAR_HEIGHT = 20;
	
	public static void main(String[] args) {
		Fence fence = new Fence();
		
	}
	
	public int generateRandom(int randomRange, int initialVal) {
		Random random = new Random();
		int randomNum = random.nextInt(randomRange) + initialVal;
		
		return randomNum;
	}
	
	public ArrayList<?> getBoardHeight(int boardCount, int boardUnderBound) {
		ArrayList<Integer> barHeights = new ArrayList<>();
		
		for(int i=0; i<boardCount; i++) {
			generateRandom(boardCount, boardUnderBound);
		}
		
		return barHeights;
	}
}
